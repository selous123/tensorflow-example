    "import collections\n",
    "\n",
    "c = collections.Counter(['a','b','c','b','c','c','a','b'])\n",
    "\n",
    "print c\n",
    "\n",
    "c.update({'a':1,'d':5})\n",
    "\n",
    "print c.items()\n",
    "\n",
    "cpairs = sorted(c.items(), key=lambda x: (-x[1],x[0]))\n",
    "\n",
    "words,_=list(zip(*cpairs))\n",
    "\n",
    "word_to_id = zip(words, range(len(words)))\n",
    "\n",
    "\n",
    "print word_to_id\n",
    "\n",
    "print c"
